{
    "name": "wennlys/no-framework-cms-backend",
    "description": "A no framework cms backend :P.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "wennlys",
            "email": "wennlys@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Source\\": "src/"
        },
        "files": [
            "src/Global/Configs.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "laminas/laminas-diactoros": "^2.3",
        "league/route": "^4.5",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "league/container": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5",
        "guzzlehttp/guzzle": "^6.5"
    }
}
